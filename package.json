{
  "name": "react-vite-project-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --mode DEV --open",
    "dev": "vite --mode DEV --open",
    "dev:sit": "vite --mode SIT --open",
    "dev:uat": "vite --mode UAT --open",
    "dev:prod": "vite --mode PROD --open",
    "build:dev": "tsc && vite build --mode DEV",
    "build:sit": "tsc && vite build --mode SIT",
    "build:uat": "tsc && vite build --mode UAT",
    "build:prod": "tsc && vite build --mode PROD",
    "test": "",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:styles": "stylelint \"src/**/*.css\" \"src/**/*.scss\" --max-warnings 0",
    "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,css,scss,json}' --write",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,scss,json,md}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix --max-warnings 0"
    ]
  },
  "engines": {
    "node": ">=18.7.0",
    "npm": ">=8.15.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/redux-logger": "^3.0.9",
    "date-fns": "^2.30.0",
    "formik": "^2.4.3",
    "i18next": "^23.2.11",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "react-use": "^17.4.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sort-by": "^0.0.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.3",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "stylelint": "^15.10.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
